{
  "name": "notification_service",
  "version": "1.0.0",
  "description": "This is the notification microservice for DevFoundryBankApp",
  "main": "dist/src/server.js",
  "type": "commonjs",
  "scripts": {
    "tsoa:routes": "tsoa routes",
    "tsoa:spec": "tsoa spec",
    "preswagger:clean": "rimraf src/swagger",
    "tsoa:update": "npm run preswagger:clean && npm run tsoa:routes && npm run tsoa:spec",
    "dev": "npm run tsoa:update && npx nodemon --exec ts-node src/server.ts",
    "build": "rimraf dist && npm run tsoa:update && rimraf dist && tsc",
    "start": "node dist/src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.5",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.12.1",
    "nodemailer": "^6.10.0",
    "nodemailer-express-handlebars": "^7.0.0",
    "path": "^0.12.7",
    "rimraf": "^6.0.1",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsoa": "^6.6.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
